{
    "name": "@akushniruk/baseapp-expo-sdk",
    "version": "0.0.34",
    "main": "lib/commonjs/index.js",
    "license": "MIT",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "update-stories": "sb-rn-get-stories --config-path .ondevice",
        "prestart": "yarn update-stories",
        "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
        "storybook": "start-storybook -p 3000",
        "build-storybook": "build-storybook",
        "serve-storybook": "serve storybook-static",
        "prepack": "bob build",
        "release": "npm publish"
    },
    "dependencies": {
        "@hookform/resolvers": "2.9.11",
        "@react-native-async-storage/async-storage": "~1.17.3",
        "@react-native-community/datetimepicker": "6.5.2",
        "@react-native-community/slider": "4.2.4",
        "@react-navigation/native": "6.1.4",
        "@reduxjs/toolkit": "1.9.2",
        "@types/i18n-js": "3.8.4",
        "expo": "^47.0.0",
        "expo-clipboard": "~4.0.1",
        "expo-localization": "14.0.0",
        "expo-status-bar": "~1.4.2",
        "i18n-js": "3.8",
        "react-dom": "18.1.0",
        "react-hook-form": "7.43.1",
        "react-native-safe-area-context": "^4.4.1",
        "react-native-web": "^0.18.10",
        "react-redux": "8.0.5",
        "redux": "4.2.1",
        "zod": "3.20.6"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@expo/webpack-config": "0.17.4",
        "@storybook/addon-actions": "^6.5.16",
        "@storybook/addon-controls": "^6.5.16",
        "@storybook/addon-essentials": "^6.5.16",
        "@storybook/addon-links": "^6.5.16",
        "@storybook/addon-ondevice-actions": "6.0.1-beta.12",
        "@storybook/addon-ondevice-backgrounds": "6.0.1-beta.12",
        "@storybook/addon-ondevice-controls": "6.0.1-beta.12",
        "@storybook/addon-ondevice-notes": "6.0.1-beta.12",
        "@storybook/addon-react-native-web": "^0.0.19",
        "@storybook/react": "^6.5.16",
        "@storybook/react-native": "6.0.1-beta.12",
        "@types/react": "~18.0.24",
        "@types/react-native": "~0.70.6",
        "babel-loader": "^8.2.3",
        "babel-plugin-react-docgen-typescript": "^1.5.1",
        "babel-plugin-react-native-web": "^0.18.10",
        "react-native-builder-bob": "^0.20.4",
        "typescript": "^4.6.3"
    },
    "peerDependencies": {
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "react-native": "0.70.5"
    },
    "react-native": "src/index.ts",
    "module": "lib/module/index.js",
    "types": "lib/typescript/src/index.d.ts",
    "files": [
        "src",
        "lib",
        "!**/__tests__",
        "!**/__fixtures__",
        "!**/__mocks__"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "commonjs",
            "module",
            "typescript"
        ]
    },
    "eslintIgnore": [
        "node_modules/",
        "lib/"
    ]
}
